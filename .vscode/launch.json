  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.js"
    }, {
      "type": "node",
      "request": "launch",
      "name": "debug unit tests",
      "program": "${workspaceFolder}/node_modules/.bin/lab",
       "args": [
           "test"
       ]
  }, {
    "type": "node",
    "request": "launch",
    "name": "processMessage",
    "program": "${workspaceFolder}/node_modules/.bin/sls",
    "args":[
      "invoke",
      "local",
      "-f",
      "processMessage",
      "--path",
      "${workspaceFolder}/test/data/fws-message.json"
    ]
  }, {
    "type": "node",
    "request": "launch",
    "name": "getAllMessages-xml",
    "program": "${workspaceFolder}/node_modules/.bin/sls",
    "args":[
      "invoke",
      "local",
      "-f",
      "getAllMessages",
      "--path",
      "${workspaceFolder}/test/data/get-all-messages-xml.json"
    ]
  }, {
    "type": "node",
    "request": "launch",
    "name": "getAllMessages-json",
    "program": "${workspaceFolder}/node_modules/.bin/sls",
    "args":[
      "invoke",
      "local",
      "-f",
      "getAllMessages",
      "--path",
      "${workspaceFolder}/test/data/get-all-messages-json.json"
    ]
  }, {
    "type": "node",
    "request": "launch",
    "name": "activate-target-area",
    "program": "${workspaceFolder}/node_modules/.bin/sls",
    "args":[
      "invoke",
      "local",
      "-f",
      "activateTargetArea",
      "--path",
      "${workspaceFolder}/test/data/get-all-messages-json.json"
    ]
  }
  ]
}